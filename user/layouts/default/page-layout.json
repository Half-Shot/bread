{
"name": "default",
"css":["default/page-layout.css"],
"elements": [
    {
        "name": "navbar",
	"id": "navbar",
        "human": "Navigation Bar",
        "description": "Top of the page, ideal for navbars.",
        "tag":"header",
        "event":"Theme.DrawNavbar",
        "elements":[
            {
                "name":"login-name",
                "id":"login-name",
                "event":"BreadUserSystem.LoginName",
                "arguments":
                {
                   "Class":"navbar-text"
                }
            },
            {
                "name":"button-squisher",
                "id":"loginbuttongroup",
                "event":"Theme.Layout.ButtonGroup",
                "elements":
                [
                    {
                        "name":"login-button",
                        "id":"login-button",
                        "event":"BreadUserSystem.LoginButton",
                        "arguments":
                        {
                           "Class":"navbar-btn"
                        }
                    },
                    {
                        "name":"controlpanel-button",
                        "id":"controlpanel-button",
                        "event":"BreadAdminTools.Button",
                        "arguments":"navbar-btn"
                    }
                ]
            },
            {
                "name": "search-bar",
                "id": "text-element",
                "event": "BreadIndexSystem.SearchForm"
            }
        ]
    },
    {
        "name": "contents",
	"id": "contents",
        "human": "Page Contents",
        "description": "Middle section where contents will be placed.",
        "tag":"section"
    },
    {
        "name": "footer",
        "id": "footer",
        "human": "Footer",
        "description": "Good place for copyright or author name.",
        "tag":"footer",
        "event": "BreadPageSystem.PlainMarkdown",
        "arguments":"####Absolutely no copyright, probably even copyleft. MIT Licence"
    }
]
}
